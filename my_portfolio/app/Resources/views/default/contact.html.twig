{% extends 'base.html.twig' %}

{% block body %}
    {% set formChildren = form.children %}

    {{ form_start(form, {'action': '/contact'}) }}
   

    {% for input in formChildren %}
        {{ form_widget(input) }}
        {{ form_label(input) }}

        <p>{{ form_errors(input) }}</p>
    {% endfor %}
    <input type="submit">
    {{ form_end(form) }}


{% endblock %}

 

{% block angularjs %}{% endblock %}